
<div class="wrapper-container">
    <div class="wrapper">
		<h1>Add Note</h1>
		   <%= form_for(@note) do |f| %>
              <h4>Title</h4><%= f.text_field :title %><br>
              <%= f.hidden_field :content, id: :post_content %>
              <div class="trix-editor-container">
              <h4>Content</h4><trix-editor input="post_content"></trix-editor>
              </div>
              <%= f.button :submit %>
           <% end %>
    </div>
</div>