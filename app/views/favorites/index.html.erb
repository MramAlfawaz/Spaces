<h1 class="h1web">Favorites WEBs</h1>
<table class="rwd-table">
  <tr>
    <th class="thweb">Name</th>
    <th class="thweb">Url</th>
    <th class="thweb" colspan="3">Setting</th>
  </tr>
      <% @favorites.each do |favorite| %>
  <tr class="trweb">
    <td class="tdweb" ><%= favorite.name %></td>
    <td class="tdweb" ><%= favorite.url %></td>
    <td class="tdweb" ><%= link_to 'Edit', edit_favorite_path(favorite) %></td>
    <td class="tdweb" ><%= link_to 'Destroy', favorite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
    <% end %>
</table>

<br> <br>
<%= link_to 'New Favorite', new_favorite_path , class: "newfav" %>
